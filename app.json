{
  "expo": {
    "name": "AfriCar",
    "slug": "AfriCar",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "carrentalfront",
    "icon": "./assets/images/logo1.png",

    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo1.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "hermes"
  }
}
